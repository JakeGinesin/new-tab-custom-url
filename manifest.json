{
  "name": "tabber",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "tabber"
  },
  "content_scripts": [
    {
      "matches":["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
      "scripts": ["background.js"]
  },
  "permissions": [
    "storage",
    "tabs"
  ]
}
